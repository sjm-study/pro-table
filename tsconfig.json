{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "lib": [
      "ES2019",
      "DOM",
    ],
    "target": "ES5",
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "noImplicitThis": false
  }
}
